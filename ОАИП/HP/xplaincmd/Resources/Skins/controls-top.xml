<?xml version="1.0" encoding="UTF-8"?>
<xplainskin name="Controls on Top" description="Skin with player bar at top, play button, share button">
	<variables>
		<var type="cdata"   name="DOCTYPE" />
		<var type="data"    name="TITLE" />
		<var type="data"    name="DESCRIPTION" />
		<var type="cdata"   name="OPENGRAPH" />
		<var type="data"    name="FAVICON" />
		<var type="data"    name="PREVIEWIMAGE" />
		<var type="cdata"   name="BODYCSS" />
		<var type="data"    name="BODYSTYLE" />
		<var type="cdata"   name="XPLAIN" />
		<var type="cdata"   name="XPLAINSCRIPTS" />
		<var type="cdata"   name="XPLAININIT" />
		<var type="bool"    name="AUDIOCONTROLS" />
		<var type="calc"    name="AUDIOVOLUME_TOP" default="<%PLAYER_PROGRESSBAR_HEIGHT%>+<%PLAYER_BUTTON_HEIGHT%>-0.1" />
		<var type="calc"    name="AUDIOINFO_TOP" default="<%PLAYER_BUTTON_HEIGHT%>*2" />
		
		<var type="bool"    name="PLAYER_CONTROLS_ALWAYSVISIBLE" default="1" />
		<var type="bool"    name="PLAYER_CONTROLS_OVERLAY" default="1" />
		<var type="percent" name="PLAYER_CONTROLS_HEIGHT" default="7" />
		<var type="color"   name="PLAYER_CONTROLS_BACKGROUND_COLOR" default="#7F000000" />
		<var type="opacity" name="PLAYER_CONTROLS_OPACITY" default="1" />
		
		<var type="percent" name="PLAYER_PROGRESSBAR_HEIGHT" default="1" />
		<var type="color"   name="PLAYER_PROGRESSBAR_COLOR" default="#FF1072C5" />
		
		<var type="percent" name="PLAYER_BUTTON_WIDTH" default="10" />
		<var type="int"     name="PLAYER_BUTTON_MINWIDTH" default="60" />
		<var type="percent" name="PLAYER_BUTTON_HEIGHT" default="6" />
		<var type="int"     name="PLAYER_BUTTON_CORNERRADIUS" default="4" />
		<var type="color"   name="PLAYER_BUTTON_BACKGROUND_HOVER" default="#FF1072C5" />
		
		<var type="color"   name="PLAYER_BUTTON_FILLCOLOR" default="#FFFFFFFF" />
		<var type="color"   name="PLAYER_BUTTON_STROKECOLOR" default="#00000000" />
		<var type="int"     name="PLAYER_BUTTON_STROKEWIDTH" default="0" />

		<var type="text"    name="BUTTON_PREVIOUS_TEXT" default="Previous Slide" />
		<var type="text"    name="BUTTON_NEXT_TEXT" default="Next Slide" />
		<var type="text"    name="BUTTON_PLAY_TEXT" default="Play Slideshow" />
		<var type="text"    name="BUTTON_STOP_TEXT" default="Stop Slideshow" />
		<var type="text"    name="BUTTON_RELOAD_TEXT" default="Reload" />
		
		<var type="text"    name="BUTTON_AUDIO_MUTE_TEXT" default="Mute" />
		<var type="text"    name="BUTTON_AUDIO_UNMUTE_TEXT" default="Unmute" />
		<var type="text"    name="BUTTON_AUDIO_UNMUTE_INFO" default="Tap or click to unmute audio." />
		<var type="text"    name="BUTTON_AUDIO_VOLUME_TEXT" default="Volume" />
		
		<var type="bool"    name="BUTTON_SHARE" default="1" />
		<var type="text"    name="BUTTON_SHARE_TEXT" default="Share" />
		<var type="int"     name="BUTTON_SHARE_WIDTH" default="36" />
		<var type="display" name="BUTTON_SHARE_FACEBOOK" default="block" />
		<var type="text"    name="BUTTON_SHARE_FACEBOOK_TEXT" default="Share on Facebook" />
		<var type="display" name="BUTTON_SHARE_TWITTER" default="block" />
		<var type="text"    name="BUTTON_SHARE_TWITTER_TEXT" default="Share on Twitter" />
		<var type="display" name="BUTTON_SHARE_LINKEDIN" default="block" />
		<var type="text"    name="BUTTON_SHARE_LINKEDIN_TEXT" default="Share on LinkedIn" />
		<var type="display" name="BUTTON_SHARE_XING" default="none" />
		<var type="text"    name="BUTTON_SHARE_XING_TEXT" default="Share on Xing" />
		<var type="display" name="BUTTON_SHARE_EMAIL" default="block" />
		<var type="text"    name="BUTTON_SHARE_EMAIL_TEXT" default="Share by Email" />
		
		<var type="text"    name="BUTTON_ABOUTHELPXPLAIN_TEXT" default="Made with HelpXplain" />
		<var type="html"    name="FALLBACK_MESSAGE" default="We are sorry, but your browser does not support HelpXplain. Please use a decent browser and turn Javascript on." />
	</variables>
	
<htmlcode><![CDATA[<%DOCTYPE%>
<html>
<head>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no" />
<meta name="apple-mobile-web-app-capable" content="yes" />
<title><%TITLE%></title>
<meta name="description" content="<%DESCRIPTION%>" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<%OPENGRAPH%>
<link rel="icon" href="<%FAVICON%>" type="image/png" />
<style type="text/css" media="screen">
<%BODYCSS%>

div.fallback {
  position: absolute;
  top: 20%;
  background: #FFF;
  opacity: 0.9;
  font-size: 2em;
  padding: 1em;
  line-height: 1em
}

div#xplplayercontrols {
	 position: fixed;
	 left: 0px;
	 top: 0;
	 width: 100%;
	 height: <%PLAYER_CONTROLS_HEIGHT%>vH;
	 background: <%PLAYER_CONTROLS_BACKGROUND_COLOR%>;
 	 opacity: <%IF:PLAYER_CONTROLS_ALWAYSVISIBLE%><%PLAYER_CONTROLS_OPACITY%>;<%/IF:PLAYER_CONTROLS_ALWAYSVISIBLE%><%IFNOT:PLAYER_CONTROLS_ALWAYSVISIBLE%>0;<%/IFNOT:PLAYER_CONTROLS_ALWAYSVISIBLE%>
     transition: opacity 0.3s;
	 padding: 0;
	 margin: 0;
	 text-align: center;
}
body:hover > div#xplplayercontrols {
	 opacity: 1;
}
div#xplplayerprogress {
	 width: 100%;
	 margin: 0;
	 padding: 0;
	 border: none;
}
div#xplplayerprogress div {
	height: <%PLAYER_PROGRESSBAR_HEIGHT%>vH;
	width: 0%;
	background: <%PLAYER_PROGRESSBAR_COLOR%>;
}

div#xplplayercontrols a {
	text-decoration: none;
}

div#xplplayercontrols .button {
  display: inline-block;
  text-decoration: none;
  padding: 0;
  margin: 0;
  width: <%PLAYER_BUTTON_WIDTH%>%;
  min-width: <%PLAYER_BUTTON_MINWIDTH%>px;
  height: <%PLAYER_BUTTON_HEIGHT%>vH;
  border-radius: <%PLAYER_BUTTON_CORNERRADIUS%>px;
}

div#xplplayercontrols .disabled {
  display: none;
}
div#xplplayerprev.disabled, div#xplplayernext.disabled {
  opacity: 0.3;
  display: inline-block;
}

div#xplplayercontrols .button:hover {
  background: <%PLAYER_BUTTON_BACKGROUND_HOVER%>;
}
div#xplplayercontrols .disabled:hover {
  background: none;
}
<%IF:AUDIOCONTROLS%>	
div#xplplayermute {
	position: absolute;
	left: 0;
	bottom: 0;
}
div#xplplayerunmute {
	position: absolute;
	left: 0;
	bottom: 0;
}
div#xplplayeraudioinfo {
	position: absolute;
	display: none;
	opacity: 0;
	left: 3vW;
	top: <%AUDIOINFO_TOP%>vH;
	width: 20vW;
	padding: 8pt;
	font-size: 0.5em;
	text-align: center;
	background: #555555;
	color: #FFFFFF;
	border-radius: 5pt;
	box-shadow: 0 0 15px #c1c1c1;
	cursor: pointer;
}
div#xplplayeraudioinfo:before {
	content: "";
	position: absolute;
	width: 0;
	height: 0;
	left: 5pt;
	top: -25pt;
	border: 20px solid transparent;
	border-bottom: 22px solid #555555;
}
#xplplayervolume {
	visibility: hidden;
	position: fixed;
	opacity: 0;
	left: 3vW;
	top: <%AUDIOVOLUME_TOP%>vH;
	height: 12vW;
	width: 4vW;
	padding: 12pt 0;
	background: <%PLAYER_CONTROLS_BACKGROUND_COLOR%>;
	border-radius: 4pt;
}
#xplplayervolume div.range {
	position: relative;
	background: #555555;
	overflow: visible;
	left: 40%;
	top: 0;
	width: 20%;
	height: 100%; 
}
#xplplayervolume div.bar {
	position: relative;
	width: 100%;
	height: 100%;
	background: #a0a0a0;
}
#xplplayervolume div.slider {
	position:absolute;
	left: 1vW;
	top: 0;
	width: 2vW;
	height: 2vW;
	background: <%PLAYER_BUTTON_FILLCOLOR%>;
	border-radius: 2vW;
	border: 0.1vW solid #FFFFFF;
}
#xplplayervolume div.slider:hover {
	background: <%PLAYER_BUTTON_BACKGROUND_HOVER%>;
	border-color: #CCCCCC;
}
div#xplplayeraudiocontrols:hover > #xplplayervolume {
	visibility: visible;
	opacity: 1;
	transition: opacity 0.3s, visibility 0.3s;
}
<%/IF:AUDIOCONTROLS%>	
<%IF:BUTTON_SHARE%>
div#xplplayershare {
	position:absolute;
	right: 0;
	top: <%PLAYER_PROGRESSBAR_HEIGHT%>vH;
	width: <%BUTTON_SHARE_WIDTH%>px;
}
div#xplplayersharepopup {
	position:fixed;
	right: 0;
	top: <%PLAYER_CONTROLS_HEIGHT%>vH;
	width: auto;
	height: auto;
	/*background: rgba(0, 0, 0, 0.8);*/
	visibility: hidden;
	opacity: 0;
	transition: opacity 0.3s, visibility 0.3s;
}

div#xplplayershare:hover > div#xplplayersharepopup {
    visibility: visible;
    opacity: 1;
    transition: opacity 0.3s, visibility 0.3s;
}

div#xplplayersharepopup a {
	cursor: pointer;
}
<%/IF:BUTTON_SHARE%>
div#xplplayercontrols svg {
	height:95%;
	width: auto;
	vertical-align: top;
}

div#xplplayercontrols path {
  fill: <%PLAYER_BUTTON_FILLCOLOR%>;
  stroke: <%PLAYER_BUTTON_STROKECOLOR%>;
  stroke-width: <%PLAYER_BUTTON_STROKEWIDTH%>px;
}
div#xplplayerloading {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 20vH;
  height: 20vH;
  margin: -10vH 0 0 -10vH;
}
div#xplplayerloading.disabled {
  display: none;	
}
svg.xplspinner {
  animation: rotate 2s linear infinite;
  width: 100%;
  height: 100%;
}
svg.xplspinner > circle {
    stroke: <%PLAYER_PROGRESSBAR_COLOR%>;
    stroke-linecap: round;
    animation: dash 1.5s ease-in-out infinite;
}
@keyframes rotate {
  100% {
    transform: rotate(360deg);
  }
}
@keyframes dash {
  0% {
    stroke-dasharray: 1, 150;
    stroke-dashoffset: 0;
  }
  50% {
    stroke-dasharray: 90, 150;
    stroke-dashoffset: -35;
  }
  100% {
    stroke-dasharray: 90, 150;
    stroke-dashoffset: -124;
  }
}
</style>
</head>
<body style="<%BODYSTYLE%>">
<%XPLAIN%>
